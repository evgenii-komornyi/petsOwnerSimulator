{
    "name": "petsownersimulator",
    "version": "1.0.0",
    "scripts": {
        "start": "expo start --dev-client",
        "android": "expo run:android",
        "ios": "expo run:ios",
        "web": "expo start --web",
        "build-debug": "npx react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res && react-native run-android",
        "build-release": "npx react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res && react-native run-android --variant=release",
        "clean": "cd android && ./gradlew clean && cd .."
    },
    "dependencies": {
        "@mindinventory/react-native-tab-bar-interaction": "^2.2.3",
        "@react-native-async-storage/async-storage": "^1.18.1",
        "@rneui/base": "^4.0.0-rc.7",
        "@rneui/themed": "^4.0.0-rc.7",
        "deprecated-react-native-prop-types": "^4.1.0",
        "expo": "~47.0.12",
        "expo-asset": "~8.7.0",
        "expo-av": "~13.0.3",
        "expo-navigation-bar": "^2.1.1",
        "expo-splash-screen": "~0.17.5",
        "expo-status-bar": "~1.4.2",
        "expo-updates": "~0.15.6",
        "lottie-react-native": "5.1.4",
        "patch-package": "^7.0.0",
        "react": "18.1.0",
        "react-native": "0.70.5",
        "react-native-animated-loader": "^1.0.0",
        "react-native-flip-card": "^3.5.7",
        "react-native-gesture-handler": "~2.8.0",
        "react-native-pager-view": "6.0.1",
        "react-native-progress": "^5.0.0",
        "react-native-reanimated": "~2.12.0",
        "react-native-reanimated-carousel": "^3.5.1",
        "react-native-safe-area-context": "^4.5.0",
        "react-native-svg": "12.1.1",
        "react-native-swipe-gestures": "^1.0.5",
        "react-native-tab-view": "^3.5.1",
        "react-router-native": "^6.4.5",
        "zustand": "^4.1.5"
    },
    "devDependencies": {
        "@babel/core": "^7.19.3"
    },
    "private": true
}
